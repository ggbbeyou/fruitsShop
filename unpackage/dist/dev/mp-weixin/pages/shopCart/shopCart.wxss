  overflow: hidden;
  width: 150rpx;
}
.allSelectText .allText {
  float: left;
}
.allSelectText > view:first-child {
  display: inline-block;
  float: left;
}
.scrollView {
  width: 750rpx;
}
.scrollView .viewbox {
  width: 900rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-transition: all .2s ease-in 0s;
  transition: all .2s ease-in 0s;
}
.scrollView .hong {
  width: 150rpx;
  background-color: #FD395B;
  color: #FFFFFF;
  text-align: center;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.shoppingcar .car-no-data {
  width: 100%;
  height: 700rpx;
  text-align: center;
  line-height: 700rpx;
  color: #999;
  font-size: 30rpx;
  vertical-align: middle;
}
.shoppingcar .car-no-data .iconfont {
  font-size: 40rpx;
  margin-right: 20rpx;
}


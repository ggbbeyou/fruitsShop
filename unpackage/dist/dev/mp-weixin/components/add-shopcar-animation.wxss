.container .shopcar-bus {
  height: 3.125rem;
  width: 3.125rem;
  position: fixed;
  left: 10%;
  top: 84%;
  border-radius: 50%;
  box-sizing: border-box;
  background-color: rgba(0, 0, 0, 0.5);
}
.container .shopcar-bus image {
  width: 2rem;
  height: 2rem;
  position: absolute;
  left: 50%;
  top: 50%;
  margin: -15px;
  margin-right: 14rpx;
}
.container .shopcar-bus .count {
  display: block;
  height: 20px;
  line-height: 20px;
  font-size: 12px;
  background: #ff4611;
  padding: 0 6px;
  border-radius: 10px;
  color: #fff;
  position: absolute;
  right: -8px;
  top: 2px;
}
.container .good_box {
  width: 30px;
  height: 30px;
  position: fixed;
  border-radius: 50%;
  overflow: hidden;
  left: 50%;
  top: 50%;
  z-index: +99;
}
.container .good_box image {
  display: block;
  width: 100%;
  height: 100%;
}

